{
  "devMode": false,
  "ignoreCodeBlockRestrictions": false,
  "preferredDateFormat": "MMMM-DD-YYYY",
  "firstWeekday": {
    "index": 1,
    "name": "Monday",
    "shortName": "Mo"
  },
  "syncInterval": 200,
  "minSyncInterval": 50,
  "maxSyncInterval": 1000,
  "enableJs": true,
  "viewFieldDisplayNullAsEmpty": true,
  "enableSyntaxHighlighting": true,
  "enableEditorRightClickMenu": true,
  "inputFieldTemplates": [],
  "buttonTemplates": [
    {
      "style": "primary",
      "label": "Search",
      "action": {
        "type": "command",
        "command": "webviewer:search"
      },
      "icon": "",
      "id": "web-search"
    },
    {
      "label": "Initiative Tracker",
      "icon": "swords",
      "style": "primary",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "Open the initiative tracker",
      "id": "open-initiative-tracker",
      "hidden": true,
      "actions": [
        {
          "type": "command",
          "command": "initiative-tracker:open-tracker"
        }
      ]
    },
    {
      "label": "Build Encounter",
      "icon": "hammer",
      "style": "default",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "Build a new encounter",
      "id": "build-new-encounter",
      "hidden": true,
      "actions": [
        {
          "type": "command",
          "command": "initiative-tracker:open-builder"
        }
      ]
    },
    {
      "label": "New Location",
      "hidden": false,
      "id": "new-location",
      "style": "primary",
      "actions": [
        {
          "type": "templaterCreateNote",
          "templateFile": "z_Templates/Template - Location.md",
          "fileName": "NewLocation",
          "folderPath": "2-Campaign/Locations",
          "openNote": true
        }
      ],
      "icon": "map-pin-plus",
      "tooltip": "Create a new location"
    },
    {
      "label": "New NPC",
      "hidden": false,
      "id": "new-npc",
      "style": "default",
      "actions": [
        {
          "type": "templaterCreateNote",
          "templateFile": "z_Templates/Template - NPC.md",
          "fileName": "NewNPC",
          "folderPath": "2-Campaign/Characters/NPCs",
          "openNote": true
        }
      ],
      "icon": "user-plus",
      "tooltip": "Create a new NPC"
    },
    {
      "label": "New PC",
      "hidden": false,
      "id": "new-pc",
      "style": "primary",
      "actions": [
        {
          "type": "templaterCreateNote",
          "templateFile": "z_Templates/Template - PC.md",
          "fileName": "NewPC",
          "folderPath": "2-Campaign/Characters/PCs",
          "openNote": true
        }
      ],
      "icon": "user-plus",
      "tooltip": "Create a new PC"
    },
    {
      "label": "New Session Notes",
      "hidden": false,
      "id": "new-session-notes",
      "style": "primary",
      "actions": [
        {
          "type": "templaterCreateNote",
          "templateFile": "z_Templates/Template - Session Notes.md",
          "fileName": "NewSessionNotes",
          "folderPath": "2-Campaign/Sessions",
          "openNote": true
        }
      ],
      "icon": "notebook-pen",
      "tooltip": "Create a note entry for your session"
    },
    {
      "label": "New Item",
      "hidden": false,
      "id": "new-item",
      "style": "default",
      "actions": [
        {
          "type": "templaterCreateNote",
          "templateFile": "z_Templates/Template - Item.md",
          "fileName": "NewItem",
          "folderPath": "2-Campaign/Items",
          "openNote": true
        }
      ],
      "icon": "shield-plus",
      "tooltip": "Create a new item"
    },
    {
      "label": "Insert new location description",
      "icon": "plus",
      "style": "primary",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "",
      "id": "insert-location-desc",
      "hidden": false,
      "actions": [
        {
          "type": "regexpReplaceInNote",
          "regexp": "<!--[^`\\[]*(`.*[^`]*)<location-desc>",
          "replacement": "$1<!--\n<location-desc>",
          "regexpFlags": "gm"
        }
      ]
    },
    {
      "label": "Remove last location description",
      "icon": "x",
      "style": "destructive",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "",
      "id": "remove-location-desc",
      "hidden": false,
      "actions": [
        {
          "type": "regexpReplaceInNote",
          "regexp": "(`{1}[V].*[^`]*)\\n<!--",
          "replacement": "<!--\n<location-desc>$1",
          "regexpFlags": "gm"
        }
      ]
    },
    {
      "label": "Insert new NPC description",
      "icon": "plus",
      "style": "primary",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "",
      "id": "insert-npc-desc",
      "hidden": false,
      "actions": [
        {
          "type": "regexpReplaceInNote",
          "regexp": "<!--[^`\\[]*(`.*[^`]*)<npc-desc>",
          "replacement": "$1<!--\n<npc-desc>",
          "regexpFlags": "gm"
        }
      ]
    },
    {
      "label": "Remove last NPC description",
      "icon": "x",
      "style": "destructive",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "",
      "id": "remove-npc-desc",
      "hidden": false,
      "actions": [
        {
          "type": "regexpReplaceInNote",
          "regexp": "(`{1}[V].*[^`]*)\\n<!--",
          "replacement": "<!--\n<npc-desc>$1",
          "regexpFlags": "gm"
        }
      ]
    },
    {
      "label": "Insert new item description",
      "icon": "plus",
      "style": "primary",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "",
      "id": "insert-item-desc",
      "hidden": false,
      "actions": [
        {
          "type": "regexpReplaceInNote",
          "regexp": "<!--[^`\\[]*(`.*[^`]*)<item-desc>",
          "replacement": "$1<!--\n<item-desc>",
          "regexpFlags": "gm"
        }
      ]
    },
    {
      "label": "Remove last item description",
      "icon": "x",
      "style": "destructive",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "",
      "id": "remove-item-desc",
      "hidden": false,
      "actions": [
        {
          "type": "regexpReplaceInNote",
          "regexp": "(`{1}[V].*[^`]*)\\n<!--",
          "replacement": "<!--\n<item-desc>$1",
          "regexpFlags": "gm"
        }
      ]
    },
    {
      "label": "Insert New Encounter",
      "icon": "plus",
      "style": "primary",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "Insert a new encounter into the table",
      "id": "insert-encounter",
      "hidden": false,
      "actions": [
        {
          "type": "regexpReplaceInNote",
          "regexp": "(`{3}encounter-table.*?)`{3}",
          "replacement": "$1---\nname: \nparty: Oz\ncreatures: []\n```",
          "regexpFlags": "gms"
        }
      ]
    },
    {
      "label": "Remove Last Encounter",
      "icon": "minus",
      "style": "destructive",
      "class": "",
      "cssStyle": "",
      "backgroundImage": "",
      "tooltip": "Remove the last encounter from the table",
      "id": "remove-encounter",
      "hidden": false,
      "actions": [
        {
          "type": "regexpReplaceInNote",
          "regexp": "(^.*)---\n(.*\\]\n)",
          "replacement": "$1",
          "regexpFlags": "gms"
        }
      ]
    }
  ],
  "excludedFolders": [
    "templates"
  ]
}